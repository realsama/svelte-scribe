    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^8.5.0",
        "@types/jest": "^27.5.2",
        "@types/js-yaml": "^4.0.5",
        "@types/node": "^18.15.11",
        "jest": "^29.5.0",
        "prettier": "^2.8.7",
        "prettier-plugin-svelte": "^2.10.0",
        "rollup-plugin-copy": "^3.4.0",
        "tslib": "^2.5.0"
    },
    "peerDependencies": {
        "svelte": "^3.x.x"
    },
    "type": "module"
